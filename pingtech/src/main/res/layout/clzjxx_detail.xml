<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:c="http://schemas.android.com/apk/res/com.pingtech"
    android:id="@+id/scrollview"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/mainmenu_bg_v"
    android:scrollbars="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp" >

                <com.pingtech.hgqw.widget.CustomTextView
                    android:id="@+id/yzjg"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    c:textColor="@color/textcolor"
                    c:textSize="18"
                    c:title="@string/Valid_results"
                    c:titleColor="@color/white"
                    c:titleSize="18" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/txfx"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="@string/Traffic_direction"
                        c:titleColor="@color/white"
                        c:titleSize="18" />

                    <ImageView
                        android:id="@+id/imageView_Traffic_direction"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginLeft="10dip" >
                    </ImageView>
                </LinearLayout>
            </LinearLayout>

            <com.pingtech.hgqw.widget.CustomTextView
                android:id="@+id/yzjg"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                c:title="@string/cardinfo"
                c:titleColor="@color/TitleInfoTextColor"
                c:titleSize="18" />

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dip"
                android:src="@drawable/bigsepline" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/cphm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="车牌号码："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/fdjh"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="发动机号："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/cllx"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="车辆类型："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/gjdq"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="国家地区："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/clpp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="车辆品牌："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/clys"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="车辆颜色："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/gs_yyz"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="公司/拥有者："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>
            </LinearLayout>

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dip"
                android:src="@drawable/bigsepline" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/jsyxm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="驾驶员姓名："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/xb"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="@string/sel_person_sex"
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/gj"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="国　　籍："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="vertical" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/csrq"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="出生日期："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/zjzl"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="@string/sel_person_cardtype"
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/zjhm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="@string/idcardnum"
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/jszh"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="驾驶证编号："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal"
                    android:visibility="gone" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/zw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="@string/sel_person_office"
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/lxfs"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="联系方式："
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal" >

                    <com.pingtech.hgqw.widget.CustomTextView
                        android:id="@+id/ssdw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        c:textColor="@color/textcolor"
                        c:textSize="18"
                        c:title="@string/goods_check_unit"
                        c:titleColor="@color/white"
                        c:titleSize="18" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/ssdw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="@string/cardphoto"
                        android:textColor="@color/white"
                        android:textSize="20sp" />

                    <ImageView
                        android:id="@+id/icpic"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dip"
                        android:scaleType="centerInside"
                        android:src="@drawable/photo" >
                    </ImageView>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dip"
            android:layout_marginTop="5dip"
            android:src="@drawable/bigsepline" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/jlyc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_common"
                android:paddingLeft="20dip"
                android:paddingRight="20dip"
                android:text="记录异常"
                android:textColor="@drawable/btn_textcolor"
                android:textSize="13sp" />

            <Button
                android:id="@+id/back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_common"
                android:paddingLeft="20dip"
                android:paddingRight="20dip"
                android:text="@string/manualinput"
                android:textColor="@drawable/btn_textcolor"
                android:textSize="13sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/xgtxfx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_common"
                android:text="@string/modifypassdict"
                android:textColor="@drawable/btn_textcolor"
                android:textSize="13sp" />

            <Button
                android:id="@+id/wpjc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_common"
                android:paddingLeft="20dip"
                android:paddingRight="20dip"
                android:text="@string/Goods_check"
                android:textColor="@drawable/btn_textcolor"
                android:textSize="13sp" />

            <Button
                android:id="@+id/sewm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/btn_common"
                android:paddingLeft="20dip"
                android:paddingRight="20dip"
                android:text="@string/wpjc_erweimasaomiao"
                android:textColor="@drawable/btn_textcolor"
                android:textSize="13sp" />
        </LinearLayout>
    </LinearLayout>

</ScrollView>